{
  "name": "Autotester",
  "description": "Chrome extension for automated web testing",
  "version": "0.1.0",
  "manifest_version": 2,

  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "<all_urls>",
    "debugger",
    "history"
  ],

  "browser_action": {},

  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },

  "content_security_policy": "script-src 'self' 'unsafe-eval' blob: filesystem: chrome-extension-resource:"
}

