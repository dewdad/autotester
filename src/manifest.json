{
  "name": "Autotester",
  "description": "Chrome extension for automated web testing",
  "version": "0.1.0",
  "manifest_version": 2,

  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "<all_urls>",
    "debugger",
    "history"
  ],

  "browser_action": { },

  "background": {
    "scripts": [
      "bundle.js"
    ],
    "persistent": true
  }
}

