{
  "name": "Simple extension",
  "description": "Simple extension for Autotester self testing",
  "version": "0.1.0",
  "manifest_version": 2,

  "permissions": [
    "tabs",
    "<all_urls>"
  ],

  "browser_action": {
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },

  "key": ""
}
