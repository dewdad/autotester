{
  "name": "autotester",
  "version": "0.1.0",
  "description": "Chrome extension for automated web testing",
  "repository": {
    "type": "git",
    "url": "git://github.com/vitalets/autotester.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "webpack": "webpack --config ./builder/webpack.config.js",
    "copy:manifest": "cpx -v src/manifest.json dist/unpacked/",
    "copy:test": "cpx -v 'test/**' dist/unpacked/test/",
    "copy:mocha": "cpx -v 'node_modules/mocha/mocha.{js,css}' dist/unpacked/mocha/",
    "copy:ui": "cpx -v 'src/ui/ui.{html,css}' dist/unpacked/ui/",
    "copy-from-selenium": "node ./builder/copy seleniumTests",
    "copy-examples": "cpx -v 'examples/**' dist/unpacked/test/",
    "watch:webpack": "npm run webpack -- --watch",
    "watch:manifest": "npm run copy:manifest -- -w",
    "watch:test": "npm run copy:test -- -w",
    "watch:mocha": "npm run copy:mocha -- -w",
    "watch:ui": "npm run copy:ui -- -w",
    "copy": "npm run clean && run-p copy:**",
    "watch": "npm run clean && run-p watch:**",
    "build": "npm run clean && npm run webpack && run-p copy:**",
    "fileserver": "node ./node_modules/selenium-webdriver/lib/test/fileserver.js"
  },
  "dependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "mocha": "^2.5.3",
    "selenium-webdriver": "^2.53.3"
  },
  "devDependencies": {
    "archiver": "^1.0.0",
    "assert": "^1.4.1",
    "chnl": "^0.1.14",
    "cpx": "^1.3.2",
    "escape-string-regexp": "^1.0.5",
    "express": "^4.14.0",
    "fs-extra": "^0.30.0",
    "gaze": "^1.1.0",
    "keymirror": "^0.1.1",
    "material-ui": "^0.15.2",
    "multer": "^1.1.0",
    "npm-run-all": "^2.3.0",
    "promises-aplus-tests": "^2.1.2",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-tap-event-plugin": "^1.0.0",
    "serve-index": "^1.8.0",
    "sinon": "^1.17.5",
    "then-chrome": "^1.0.5",
    "webpack": "^1.13.1"
  },
  "license": "MIT"
}
